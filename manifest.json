{
   "background": {
      "scripts": [ "js/db.js" ]
   },
   "browser_action": {
      "default_icon": "img/icon48.png",
      "default_title": "Nuke Your Tracks"
   },
   "description": "Delete your browsing data based on your preferences with one click of a button. Cover your tracks and browse in peace!",
   "icons": {
      "128": "img/icon128.png",
      "16": "img/icon16.png",
      "48": "img/icon48.png"
   }, 

   "manifest_version": 2,
   "minimum_chrome_version": "19",
   "name": "Nuke Your Tracks",
   "options_page": "options.html",
   "permissions": [ "notifications", "browsingData", "storage" ],
   "version": "1.4"
}
